<h1>AdminUsers List:</h1>
<div>Total : <%= pluralize(@admin_users.size, 'admin user') %></div>
<table>
  <tr>
    <th>Username</th>
    <th>First Name</th>
    <th>Last Name</th>
    <th>Actions</th>
  </tr>
<% @admin_users.each do |admin_user| %>
<tr>
  <td><%= admin_user.username %></td>
  <td><%= admin_user.first_name %></td>
  <td><%= admin_user.last_name %></td>
  <td><%= link_to("Edit", edit_admin_user_path(admin_user))%>
    <%= link_to("Delete", delete_admin_user_path(admin_user))%>
  </td>
</tr>
<% end %>
</table>
<br/>
<div class="create admin_user">
  <%= link_to("Create new AdminUser",new_admin_user_path) %>
</div>
